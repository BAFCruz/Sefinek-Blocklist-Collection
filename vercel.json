{
  "name": "blocklist.sefinek.net",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "www/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/blocklist/generated/(.*)",
      "dest": "/blocklist/generated/$1"
    },
    {
      "src": "/www/public/(.*)",
      "dest": "/www/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}