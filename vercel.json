{
  "name": "blocklist.sefinek.net",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "www/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "www/public/$1"
    }
  ]
}